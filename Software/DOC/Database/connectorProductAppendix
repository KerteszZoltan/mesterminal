CREATE TABLE `mesterminal`.`productappendixs` ( 
    `productAndAppendixId` INT NOT NULL AUTO_INCREMENT , 
    `appendixId` INT NOT NULL ,
    `productId` INT NOT NULL ,
    PRIMARY KEY (`productAndAppendixId`),
    FOREIGN KEY (appendixId) REFERENCES appendixs(appendixId),
    FOREIGN KEY (productId) REFERENCES products(productId)
) 
ENGINE = InnoDB CHARSET=utf8 COLLATE utf8_hungarian_ci;