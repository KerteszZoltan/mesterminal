CREATE TABLE `mesterminal`.`workeroperations` ( 
    `workerAndOperationId` INT NOT NULL AUTO_INCREMENT , 
    `workerId` INT NOT NULL , 
    `operationId` INT NOT NULL , 
    `operationTime` INT NOT NULL , 
    PRIMARY KEY (`workerAndOperationId`),
    FOREIGN KEY (workerId) REFERENCES workers(workerId),
    FOREIGN KEY (operationId) REFERENCES operations(operationId)
    ) 
ENGINE = InnoDB;