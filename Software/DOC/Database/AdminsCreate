CREATE TABLE `mesterminal`.`admins` 
( `AID` INT NOT NULL AUTO_INCREMENT ,
 `Email` VARCHAR(100) CHARACTER SET utf8 COLLATE utf8_hungarian_ci NOT NULL ,
 `APassworld` VARCHAR(255) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL , 
  PRIMARY KEY (`AID`)) ENGINE = InnoDB CHARSET=utf8 COLLATE utf8_hungarian_ci;